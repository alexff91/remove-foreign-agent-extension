{
  "name": "Foreign Agent Remover",
  "description": "Extension removes annoying message about foreign agent",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "all_frames": true,
      "js": ["jquery.min.js", "background.js"],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "/images/16.png",
    "32": "/images/32.png",
    "48": "/images/48.png",
    "128": "/images/128.png"
  }
}
